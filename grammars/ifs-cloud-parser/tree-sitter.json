{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "ifs_cloud_parser",
      "camelcase": "IfsCloudParser",
      "title": "IFS Cloud Parser",
      "scope": "source.ifs_cloud",
      "file-types": [
        "entity",
        "plsql",
        "fragment",
        "client",
        "plsvc",
        "views",
        "enumeration",
        "storage",
        "projection"
      ],
      "injection-regex": "^ifs_cloud_parser$",
      "class-name": "TreeSitterIfsCloudParser"
    }
  ],
  "metadata": {
    "version": "0.4.0",
    "license": "MIT",
    "description": "Parses the IFS Cloud source code, including nested SQL support.",
    "authors": [
      {
        "name": "Sindre van der Linden",
        "email": "sindre@linden.no",
        "url": "https://www.linkedin.com/in/sindre-v-b8a3269b/"
      }
    ],
    "links": {
      "repository": "https://github.com/graknol/ifs-parser"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}