{
  "name": "ifs-cloud-parser",
  "version": "0.2.0",
  "description": "A parser for IFS Cloud source code, made with tree-sitter. Tested on the entire 25.1.0 code-base with 100% success rate. Native support for SQL included.",
  "keywords": [
    "parser",
    "lexer",
    "tree-sitter",
    "plsql",
    "ifs",
    "oracle"
  ],
  "author": "Sindre van der Linden",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "generate": "tree-sitter generate",
    "full-codebase-test": "cd ../.. && cargo test test_full_ifs_codebase_parsing -- --nocapture",
    "build": "npm run generate",
    "package": "node scripts/package.js package",
    "publish": "node scripts/package.js publish",
    "clean": "rm -rf dist/ .venv-build bindings/*/build bindings/*/dist bindings/*/*.egg-info"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "jest": "^30.0.5",
    "tree-sitter-cli": "^0.25.0"
  },
  "tree-sitter": [
    {
      "scope": "source.ifs.plsql",
      "file-types": [
        "plsql"
      ],
      "injection-regex": "plsql-ifs"
    }
  ],
  "main": "bindings/node"
}